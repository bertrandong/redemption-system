generator client {
  provider = "prisma-client-js"
  seed = "prisma/seed.ts"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  staff_pass_id String @id @unique
  team_name String
  created_at DateTime
}

model Redemption {
  team_name String @unique
  redeemed_at DateTime?
}
